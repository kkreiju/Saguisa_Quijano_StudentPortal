@{
	ViewData["Title"] = "Search Results";
}

@model List<StudentPortal.Models.Entities.Students>

<h1>Results</h1>

<table class="table">
	<thead>
		<tr>
			<th scope="col">ID</th>
			<th scope="col">Name</th>
			<th scope="col">Course</th>
			<th scope="col">Year</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var student in Model)
		{
			<tr>
				<th scope="row">@student.StudID</th>
				<td>@student.StudFName @student.StudLName</td>
				<td>@student.StudCourse</td>
				<td>@student.StudYear</td>
			</tr>
		}
	</tbody>
</table>


